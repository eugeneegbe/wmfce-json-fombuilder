{
	"method": "get",
	"class": ["class-this class-that class-end"], 
	"id": "elt_id", 
	"schema": [
		{
			"elt": "input", 
			"label": "Input number ",
			"type": "number", 
			"attribute": {"autocomplete": "true", "multiple": "false"},
			"default": 12, 
			"class": "form-control elt-class", 
			"id": "textInput1"
		}, 
		{
			"elt": "select", 
			"label": "Select with multiple elements",
