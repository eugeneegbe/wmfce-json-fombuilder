			"default": "default-value", 
			"class": "select-autocomplete elt-class", 
			"id": "selectMultipleInput", 
			"spacing": "form-check",
			"options": {
				"Digital Renter": "Digital Renter", 
				"Makonjo": "Makonjo", 
				"Njorku": "Njorku", 
				"Maibeta": "Maibeta"
			}
		},
		{
			"elt": "select", 
			"label": "Select with autocmplete feature",
			"default": "default-value", 
			"class": "select-autocomplete", 
			"id": "selectAutocompleteInput", 
			"spacing": "form-check",
			"options": {
				"Cameroon": "Cameroon", 
				"Nigeria": "Nigeria", 
				"Ghana": "Ghana", 
				"Ethiopia": "Ethiopia"
			}
		}, 
		{
			"elt": "radio", 
			"spacing": "form-check",
			"label": "Radio button element",
			"attribute": {"name": "name-of-radio"},
			"default": "default-value", 
			"class": "form-control elt-class", 
			"id": "radioInput", 
			"options": {
