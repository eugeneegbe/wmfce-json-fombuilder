				"handball": "handball", 
				"table tenis": "table tenis", 
				"judo": "judo"
			}
		}, {
			"elt": "checkbox", 
			"label": "Checkbox element",
			"attribute": {"name": "name-of-checkbox"},
			"default": "default-value", 
			"class": "form-control elt-class", 
			"id": "checkboxInput", 
			"options": {
				"Tom": "Tom", 
				"Dick": "Dick", 
				"Haryy": "Haryy", 
				"Paul": "Paul"
			}
		}, {
			"elt": "textarea", 
			"label": "Textarea element",
			"attribute": {"name": "textarea_field"},
			"default": "This is the defualt text that will apear on the text area field", 
			"class": "form-control elt-class", 
			"id": "textareaInput"
		}, {
			"elt": "input",
			"label": "",
